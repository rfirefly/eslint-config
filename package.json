  },
  "devDependencies": {
    "@rfirefly/eslint-config": "*",
    "bumpp": "^8.2.1",
    "eslint": "^8.33.0",
    "eslint-plugin-antfu": "workspace:*",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.4"
  }
}
