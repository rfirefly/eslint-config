{
  "name": "@rfirefly/eslint-config-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@7.1.0",
  "author": "rfirefly",
  "license": "MIT",
  "scripts": {
    "lint": "eslint .",
    "test": "pnpm -r run test",
